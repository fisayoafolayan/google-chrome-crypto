{
  "manifest_version": 2,
  "name": "Cryptowatcher",
  "description": "A simple application to track live cryptocurrency prices",
  "version": "1.0",
  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "index.html",
     "default_title": "Cryptowatcher"
  },
  "permissions": [
     "activeTab"
  ]
}